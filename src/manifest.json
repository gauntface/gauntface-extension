{
  "name": "Gauntface Extension",
  "description": "An extension that enables you to configure preset pinned tabs, preset pop-up windows and a custom tab page.",
  "version": "1.3.17",
  "manifest_version": 2,
  "options_page": "options.html",
  "icons": {
    "64": "images/icons/favicon-64x64.png",
    "128": "images/icons/favicon-128x128.png",
    "512": "images/icons/favicon-512x512.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "key": "This key is update by the gulp build script"
}