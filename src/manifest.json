{
  "name": "Gauntface Extension",
  "description": "Personal Extension for @gauntface",
  "version": "1.3.2",
  "manifest_version": 2,
  "options_page": "options.html",
  "icons": {
    "64": "images/icons/64x64.png",
    "128": "images/icons/128x128.png",
    "512": "images/icons/512x512.png"
  },
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}